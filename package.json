{
  "name": "multitrack-music-remixer",
  "version": "1.0.0",
  "description": "A professional web-based multi-track music remixing application inspired by fadr.com/remix. This application allows users to upload, edit, and remix audio tracks with professional-grade tools and effects.",
  "main": "server/index.ts",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon --watch server --exec ts-node server/index.ts",
    "client:dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "test": "jest"
  },
  "keywords": [
    "music",
    "remixer",
    "audio",
    "multitrack",
    "web",
    "react",
    "typescript"
  ],
  "author": "Kaaneii Williams",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.44.0",
    "express": "^4.18.2",
    "mongodb": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tone": "^14.8.39",
    "wavesurfer.js": "^7.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.5",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
